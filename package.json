{
  "name": "yabbyville-monorepo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=packages/web",
    "dev:web": "npm run dev --workspace=packages/web",
    "dev:auth": "npm run dev --workspace=packages/copyparty-auth",
    "dev:monitor": "npm run dev --workspace=packages/file-mover",
    "dev:all": "concurrently \"npm run dev:web\" \"npm run dev:auth\" \"npm run dev:monitor\"",
    "build": "npm run build --workspaces --if-present",
    "build:web": "npm run build --workspace=packages/web",
    "lint": "eslint .",
    "preview": "npm run preview --workspace=packages/web"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "concurrently": "^9.1.2",
    "eslint": "^9.30.1",
    "globals": "^16.3.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1"
  }
}
